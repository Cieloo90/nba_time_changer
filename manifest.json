{
  "manifest_version": 2,
  "name": "nba.com time changer",
  "version": "0.1",
  "permissions": ["activeTab", "tabs"],
  "browser_action": {
    "default_icon": "images/inactive.png",
    "default_title": "nba.com time changer"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "icons": {
    "32": "images/active.png",
    "32": "images/inactive.png"
  }
}